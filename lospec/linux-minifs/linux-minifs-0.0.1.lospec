package = {
	name     = "linux-minifs",
	version  = "0.0.1",
	source   = "$(repo.opensource)/prebuilt-toolchains/linux-minifs.tar.bz2"
}

function installafter()
	local src = makepathdir(path.src.dir, "linux-minifs")
	local dst = makepathdir(path.install.dir)
	copydir(src, dst)
	return true
end

use "prebuilt"
